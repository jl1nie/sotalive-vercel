{
  "name": "myact",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "VITEST_INTEGRATION=true vitest run --config vitest.integration.config.ts src/test/integration/api.integration.test.ts",
    "test:e2e": "VITEST_E2E=true vitest run src/test/integration/e2e.test.ts",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:debug": "playwright test e2e/spot-card-debugging.spec.ts --timeout=30000",
    "test:e2e:quick": "pkill -f vite || true && timeout 30s npm run dev & sleep 3 && npx playwright test e2e/spot-card-debugging.spec.ts --timeout=30000 --workers=1",
    "test:prod": "VITEST_INTEGRATION=true VITEST_E2E=true vitest run src/test/integration",
    "test:all": "npm run test:run && npm run test:integration && npm run test:e2e",
    "test:pyramid:unit": "vitest run",
    "test:pyramid:integration": "vitest run --config vitest.integration.config.ts",
    "test:pyramid:e2e": "playwright test",
    "test:pyramid:all": "npm run test:pyramid:unit && npm run test:pyramid:integration && npm run test:pyramid:e2e",
    "test:pyramid:coverage": "vitest run --coverage && vitest run --config vitest.integration.config.ts --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/x-date-pickers": "^8.6.0",
    "@tanstack/react-query": "^5.81.5",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.19",
    "@types/topojson-client": "^3.1.5",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-leaflet": "^5.0.0",
    "react-virtualized": "^9.22.6",
    "recharts": "^3.0.2",
    "topojson-client": "^3.1.0",
    "zustand": "^5.0.6"
  }
}
