{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/logconv.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/logconv/(.*)",
            "dest": "/api/logconv.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/server.js"
        }
    ]
}