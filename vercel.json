{
    "functions": {
        "api/logconv.py": {
            "memory": 1024
        }
    },
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/api/logconv/:match*",
            "destination": "/api/logconv.py"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "myact.sotalive.net"
                }
            ],
            "destination": "/myact/:match*"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "mypilgrim.sotalive.net"
                }
            ],
            "destination": "/myact/:match*"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "myqth.sotalive.net"
                }
            ],
            "destination": "/myqth/:match*"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "logconv.sotalive.net"
                }
            ],
            "destination": "/logconv/:match*"
        },
        {
            "source": "/logconv/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "www.sotalive.net"
                }
            ],
            "destination": "/logconv/:match*"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "www.sotalive.net"
                }
            ],
            "destination": "/myact/:match*"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "myact.vercel.app"
                }
            ],
            "destination": "/myact/:match*"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "myqth.vercel.app"
                }
            ],
            "destination": "/myqth/:match*"
        },
        {
            "source": "/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "logconv.vercel.app"
                }
            ],
            "destination": "/logconv/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/404.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "myact.sotalive.net",
                    "not": true
                },
                {
                    "type": "host",
                    "value": "mypilgrim.sotalive.net",
                    "not": true
                },
                {
                    "type": "host",
                    "value": "myqth.sotalive.net",
                    "not": true
                },
                {
                    "type": "host",
                    "value": "logconv.sotalive.net",
                    "not": true
                },
                {
                    "type": "host",
                    "value": "www.sotalive.net",
                    "not": true
                },
                {
                    "type": "host",
                    "value": "myact.vercel.app",
                    "not": true
                },
                {
                    "type": "host",
                    "value": "myqth.vercel.app",
                    "not": true
                },
                {
                    "type": "host",
                    "value": "logconv.vercel.app",
                    "not": true
                },
                {
                    "type": "path",
                    "value": "/api/logconv/(.*)",
                    "not": true
                },
                {
                    "type": "path",
                    "value": "/",
                    "not": true
                }
            ],
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=172800, s-maxage=172800"
                }
            ]
        }
    ]
}